+static int
+get_expected_filecontext(const char *path, char **result)
+{
+	static struct selabel_handle *hdl;
+
+	if (!hdl) {
+		static bool disabled;
+		if (disabled)
+			return -1;
+
+		hdl = selabel_open(SELABEL_CTX_FILE, NULL, 0);
+		if (!hdl) {
