+				   "context mismatch checking");
+			disabled = true;
+			return -1;
+		}
+	}
+
+	strace_stat_t stb;
+	if (stat_file(path, &stb) < 0) {
+		return -1;
+	}
+
+	char *secontext;
+	return getcontext(selabel_lookup(hdl, &secontext, path, stb.st_mode),
+			  &secontext, result);
